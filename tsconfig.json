{
  "compilerOptions": {
    "target": "es6",
    "jsx": "react-jsx",
    "module": "nodenext",
    "declaration": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "moduleResolution": "nodenext",
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true
  }
}
